generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Property {
  id          String   @id @default(cuid())
  title       String
  description String?
  location    String
  price       Decimal
  size        Float 
  openArea    Float 
  plantedArea Float?
  area_lavoura  Float?
  imageUrl    String
  imageUrls   String?
  isVerified  Boolean  @default(false)
  distance    String?
  soilType    String?
  features    String?
  cond_pag     String?
  status      String?
  camp_safra  Boolean @default(false)
  camp_videos   String?
}

